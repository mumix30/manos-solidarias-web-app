@model IReadOnlyList<ManosSolidarias.Mvc.Entities.Organizacion>
@{
    ViewData["Title"] = "Organizaciones";
}

<section class="page">
    <div class="page-header">
        <h1>Organizaciones</h1>
        <a class="button" asp-action="Create">Nueva organización</a>
    </div>

    <table class="table">
        <thead>
            <tr>
                <th>Organización</th>
                <th>Contacto</th>
                <th>Correo</th>
                <th>Estatus</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var org in Model)
            {
                <tr>
                    <td>@org.Organizacion</td>
                    <td>@org.Telefono</td>
                    <td>@org.Email</td>
                    <td>@org.Estatus</td>
                </tr>
            }
        </tbody>
    </table>
</section>
